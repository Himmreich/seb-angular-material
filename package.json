{
  "name": "seb-ng-material",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "theme:compile": "node-sass --output-style=compressed projects/seb-ng-material-theme/seb-mat-theme.scss projects/seb-ng-material-theme/seb-mat-theme.css",
    "theme:docs": "copyfiles README.md LICENSE.md projects/seb-ng-material-theme",
    "theme:fonts": "copyfiles -f \"./node_modules/@sebdesignlibrary/visual-identity/src/fonts/*\" \"./projects/seb-ng-material-theme/assets/fonts\"",
    "theme:pack": "npm run theme:compile && npm run theme:fonts && npm run theme:docs",
    "theme:release": "cd projects/seb-ng-material-theme && branch-ng-tools publish"
  },
  "repository": {
    "type": "git",
    "url": "https://github.sebank.se/BranchChannel/seb-angular-material-theme"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~7.1.0",
    "@angular/cdk": "^7.2.0",
    "@angular/common": "~7.1.0",
    "@angular/compiler": "~7.1.0",
    "@angular/core": "~7.1.0",
    "@angular/forms": "~7.1.0",
    "@angular/material": "^7.2.0",
    "@angular/platform-browser": "~7.1.0",
    "@angular/platform-browser-dynamic": "~7.1.0",
    "@angular/router": "~7.1.0",
    "@sebdesignlibrary/visual-identity": "^3.0.2",
    "core-js": "^2.5.4",
    "rxjs": "~6.3.3",
    "showdown": "^1.9.0",
    "tslib": "^1.9.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.11.0",
    "@angular/cli": "~7.1.4",
    "@angular/compiler-cli": "~7.1.0",
    "@angular/language-service": "~7.1.0",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "branch-ng-tools": "^0.2.1",
    "codelyzer": "~4.5.0",
    "copyfiles": "^2.1.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.1.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.1.6"
  }
